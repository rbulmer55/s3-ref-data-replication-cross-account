{
	"name": "s3-replication-data-cross-account",
	"version": "0.1.0",
	"bin": {
		"s3-replication-data": "bin/s3-replication-data.js"
	},
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"test": "jest",
		"cdk": "cdk",
		"deploy": "npm run deploy-account-c && npm run deploy-account-b && npm run deploy-account-a",
		"deploy-account-a": "cdk deploy --profile primary S3ReplicationDataStackStatefulA S3ReplicationDataStackStatelessA --exclusively",
		"deploy-account-b": "cdk deploy --profile serviceone S3ReplicationDataStackStatefulB --exclusively",
		"deploy-account-c": "cdk deploy --profile servicetwo S3ReplicationDataStackStatefulC --exclusively"
	},
	"devDependencies": {
		"@types/aws-lambda": "^8.10.115",
		"@types/jest": "^29.4.0",
		"@types/node": "18.14.6",
		"aws-cdk": "2.72.1",
		"jest": "^29.5.0",
		"ts-jest": "^29.0.5",
		"ts-node": "^10.9.1",
		"typescript": "~4.9.5"
	},
	"dependencies": {
		"ajv": "^8.12.0",
		"aws-cdk-lib": "2.72.1",
		"aws-sdk": "^2.1381.0",
		"constructs": "^10.0.0",
		"source-map-support": "^0.5.21"
	}
}
